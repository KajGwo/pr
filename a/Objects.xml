<?xml version="1.0" encoding="UTF-8"?>
<geometricFigures>
    <figure>
        <name>Circle</name>
        <type>2D</type>
        <properties>
            <property>Radius</property>
            <property>Diameter</property>
        </properties>
        <formulas>
            <area>π * radius^2</area>
            <perimeter>2 * π * radius</perimeter>
        </formulas>
    </figure>
    <figure>
        <name>Square</name>
        <type>2D</type>
        <properties>
            <property>Side Length</property>
        </properties>
        <formulas>
            <area>side^2</area>
            <perimeter>4 * side</perimeter>
        </formulas>
    </figure>
<figure>
    <name>Cube</name>
    <type>3D</type>
    <properties>
     <property>Depht</property>
     <property>Lenght</property>
     <property>Height</property>
    </properties>
    <formulas>
     <volume>Lenght * Depht * Height</volume>
     <surface>(Lenght * Height) * 6</surface>
    </formulas>
</figure>
</geometricFigures>